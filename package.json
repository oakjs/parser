{
  "repository": "https://github.com/oakjs/parser",
  "license": "MIT",
  "scripts": {
    "start": "nodemon server.js --exec babel-node",
    "watch": "parcel watch src/index.html --out-dir build",
    "build": "parcel build src/index.html --out-dir dist --experimental-scope-hoisting",
    "pserve": "parcel serve src/index.html --out-dir build --no-hmr",
    "prettier": "prettier --write \"src/**/*.js*\"",
    "eslint": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "clean": "rm -rf ./.cache; rm -rf ./build"
  },
  "jest": {
    "moduleNameMapper": {
      "~/(.*)": "<rootDir>/src/$1"
    }
  },
  "nodemonConfig": {
    "watch": [
      "server.js",
      "src/server/"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@reach/router": "^1.3.4",
    "@risingstack/react-easy-state": "^6.3.0",
    "any-base": "^1.1.0",
    "assert": "^2.0.0",
    "babel-eslint": "^10.0.3",
    "body-parser": "^1.19.0",
    "browser-or-node": "^1.1.0",
    "chalk": "^2.4.2",
    "classnames": "^2.2.6",
    "codemirror": "^5.48.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "express": "^4.16.4",
    "express-json5": "^0.1.0",
    "fs-extra": "^7.0.1",
    "global": "^4.3.2",
    "history": "^4.7.2",
    "jshint": "^2.10.2",
    "json5": "^2.1.1",
    "languages": "^0.1.3",
    "lodash": "^4.17.11",
    "lodash-inflection": "^1.5.0",
    "mime-types": "^2.1.24",
    "node-filter-async": "^2.0.0",
    "parcel-bundler": "^1.12.4",
    "path": "^0.12.7",
    "prop-types": "^15.5.8",
    "proper-lockfile": "^3.2.0",
    "query-string": "^6.12.1",
    "react": ">=15",
    "react-codemirror2": "^6.0.0",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-hotkeys-hook": "^2.1.3",
    "react-keydown": "^1.7.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.5.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "eslint": "6.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-prettier-react": "0.0.24",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "^1.3.0",
    "jest": "^25.4.0",
    "less": "^3.9.0",
    "nodemon": "^1.18.10"
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime"
        ]
      }
    },
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-do-expressions",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/*",
    "unused/*",
    "thoughts/*"
  ],
  "eslintConfig": {
    "env": {
      "es6": true,
      "jest": true,
      "shared-node-browser": true
    },
    "extends": [
      "airbnb-base",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "globals": {
      "alert": "readonly",
      "confirm": "readonly",
      "document": "readonly",
      "fetch": "readonly",
      "prompt": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "classes": true,
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "prettier",
      "react"
    ],
    "rules": {
      "camelcase": "off",
      "class-methods-use-this": "off",
      "func-names": "off",
      "import/no-unresolved": "off",
      "import/no-cycle": "off",
      "import/prefer-default-export": "off",
      "import/extensions": [
        1,
        "never",
        {
          "css": "always"
        }
      ],
      "indent": [
        "error",
        2,
        {
          "ignoredNodes": [
            "TemplateLiteral"
          ],
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "lines-between-class-members": "off",
      "max-len": [
        "off",
        {
          "code": 120,
          "tabWidth": 2
        }
      ],
      "no-alert": "off",
      "no-cond-assign": "off",
      "no-multi-assign": "off",
      "no-plusplus": "off",
      "no-restricted-globals": "off",
      "no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "args": "none",
          "ignoreRestSiblings": false
        }
      ],
      "no-underscore-dangle": "off",
      "no-use-before-define": "off",
      "no-useless-escape": "off",
      "no-console": "off",
      "no-constant-condition": "off",
      "no-empty-function": "off",
      "no-param-reassign": "off",
      "prettier/prettier": "warn",
      "prefer-destructuring": "off",
      "semi": [
        "error",
        "never"
      ],
      "strict": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "prettier": {
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
